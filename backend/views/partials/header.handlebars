<ul id="header">
  <a href="/">
    <li class="float-left ml-5">Home</li>
  </a>
  {{#if isUserLogged}}

  <a href="/memo">
    <li class="float-left ml-5">Memo</li>
  </a>

  <a href="/logout">
    <li class="float-right mr-5">Logout</li>
  </a>

  <li id="header-username" class="float-right mr-1">{{username}}</li>

  {{else}}
  <a href="/login">
    <li class="float-right mr-5">Login</li>
  </a>
  <a href="/register">
    <li class="float-right">Register</li>
  </a>
  {{/if}}
</ul>